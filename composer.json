{
  "name": "hirnsturm/docker-typo3",
  "description": "",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Steve Lenz",
      "email": "kontakt@steve-lenz.de",
      "role": "Technical Project Manager"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    }
  ],
  "require": {
    "typo3/cms": "^7.6",
    "typo3-ter/typo3-console": "^3.4"
  },
  "require-dev": {
    "typo3-ter/extension-builder": "~7.6"
  },
  "config": {
    "vendor-dir": "htdocs/vendor"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": "./htdocs/web"
    }
  }
}
